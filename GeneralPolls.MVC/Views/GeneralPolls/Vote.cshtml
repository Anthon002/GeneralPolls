@model GeneralPolls.Core.DTOs.RegisteredVotersViewModel;

<p>
    You are about to vote for This @ViewData["CandidateName"]
</p>
<form asp-action="Vote" asp-controller="GeneralPolls" method="post">
    <input asp-for="Id" value="@Model.Id" type="hidden"/>
    <input asp-for="ElectionId" value="@Model.ElectionId" type="hidden" />
    <input asp-for="UserId" value="@Model.UserId" type="hidden" />
    <input asp-for="Vote" value="@Model.Vote" type="hidden" />
    <input type="submit" value="vote" />
</form>
